{
  "name": "sonarqube-companion-frontend",
  "version": "2.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "node node_modules/.bin/ng",
    "ngc": "node node_modules/.bin/ngc",
    "ts-node": "node node_modules/.bin/ts-node",
    "lint": "node node_modules/.bin/ng lint --force=true --format=json | sed 's/Linting.*$//' > target/tslint-report.json",
    "test": "yarn ng test --single-run --browsers PhantomJS",
    "e2e": "yarn ng e2e --proxy-config proxy.local.conf.json",
    "build": "yarn ng build --prod",
    "start": "yarn start-local",
    "start-local": "yarn ng serve --host 0.0.0.0 --proxy-config proxy.local.conf.json",
    "postinstall": "ngcc"
  },
  "private": true,
  "engines": {
    "yarn": ">=1.0.0"
  },
  "dependencies": {
    "@angular/animations": "10.1.0",
    "@angular/cdk": "7.0.3",
    "@angular/common": "10.1.0",
    "@angular/compiler": "10.1.0",
    "@angular/core": "10.1.0",
    "@angular/forms": "10.1.0",
    "@angular/platform-browser": "10.1.0",
    "@angular/platform-browser-dynamic": "10.1.0",
    "@angular/router": "10.1.0",
    "@circlon/angular-tree-component": "10.0.0",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "moment": "2.21.0",
    "node-sass": "^4.13.1",
    "rxjs": "6.5.4",
    "spinkit": "1.2.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "10.1.0",
    "@angular/compiler-cli": "10.1.0",
    "@angular/language-service": "10.1.0",
    "@consdata/tslint-config": "1.13.0",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-phantomjs-launcher": "1.0.4",
    "protractor": "~7.0.0",
    "ts-node": "7.0.1",
    "tslint": "~6.1.0",
    "typescript": "4.0.2"
  }
}
